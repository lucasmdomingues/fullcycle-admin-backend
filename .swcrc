{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true
    },
    "target": "es2022",
    "transform": {
      "useDefineForClassFields": false
    }
  },
  "module": {
    "type": "es6"
  },
  "sourceMaps": true,
  "minify": false
}
